

library(jsonlite)

# library
library(jsonlite)

loopJson= function(input_string)
{
  # Convert the string to a list
  my_list= fromJSON(input_string)
  
  # Print the list
  for (v in my_list$ano)
  {
    print("---")
    print(v)
  }
  
}

